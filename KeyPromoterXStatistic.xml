<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close Others">
        <Item>
          <IdeaActionID>CloseAllEditorsButActive</IdeaActionID>
          <ShortCut>'⇧⌘W'</ShortCut>
          <Description>Close Others</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Reference">
        <Item>
          <IdeaActionID>CopyReference</IdeaActionID>
          <ShortCut>'⌥⇧⌘C'</ShortCut>
          <Description>Copy Reference</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘Button1 Click' or 'Button2 Click' or '⌃⌥⇧⌘H'</ShortCut>
          <Description>Declaration</Description>
          <Count>56</Count>
        </Item>
      </Statistic>
      <Statistic Action="Exclude">
        <Item>
          <IdeaActionID>UsageView.Exclude</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Exclude</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Action...">
        <Item>
          <IdeaActionID>GotoAction</IdeaActionID>
          <ShortCut>'⇧⌘A'</ShortCut>
          <Description>Find Action...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12' or '⌃⌥⇧R'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reformat with Prettier">
        <Item>
          <IdeaActionID>ReformatWithPrettierAction</IdeaActionID>
          <ShortCut>'⌥⌘L'</ShortCut>
          <Description>Reformat with Prettier</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Remove">
        <Item>
          <IdeaActionID>UsageView.Remove</IdeaActionID>
          <ShortCut>'⌘X' or '⇧⌦'</ShortCut>
          <Description>Remove</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resolve Conflicts...">
        <Item>
          <IdeaActionID>Git.ResolveConflicts</IdeaActionID>
          <ShortCut>'⌥⌘G'</ShortCut>
          <Description>Resolve Conflicts...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Default Layout">
        <Item>
          <IdeaActionID>RestoreDefaultLayout</IdeaActionID>
          <ShortCut>'⇧F12'</ShortCut>
          <Description>Restore Default Layout</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'⌥⌘Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>