<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘Button1 Click' or 'Button2 Click' or '⌃⌥⇧⌘H'</ShortCut>
          <Description>Declaration</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reformat with Prettier">
        <Item>
          <IdeaActionID>ReformatWithPrettierAction</IdeaActionID>
          <ShortCut>'⌥⌘L'</ShortCut>
          <Description>Reformat with Prettier</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>